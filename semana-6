CREATE TABLE `ventas` (
  `id_ventas` int NOT NULL AUTO_INCREMENT,
  `fechaa` timestamp NULL DEFAULT NULL,
  `total_venta` float DEFAULT '0',
  `Clientes_id_cliente` int NOT NULL,
  PRIMARY KEY (`id_ventas`),
  KEY `fkVentas_Clientes` (`Clientes_id_cliente`),
  CONSTRAINT `fkVentas_Clientes` FOREIGN KEY (`Clientes_id_cliente`) REFERENCES `clientes` (`id_cliente`)
) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci


CREATE TABLE `detalles_ventas` (
  `id_Detalle_Venta` int NOT NULL AUTO_INCREMENT,
  `cantidad_producto` float DEFAULT NULL,
  `precio_unitario` float DEFAULT NULL,
  `total_x_prodcuto` float GENERATED ALWAYS AS ((`cantidad_producto` * `precio_unitario`)) VIRTUAL,
  `Ventas_id_ventas` int NOT NULL,
  `Productos_id_producto` int NOT NULL,
  PRIMARY KEY (`id_Detalle_Venta`),
  KEY `fkDetalles_Ventas_Ventas` (`Ventas_id_ventas`),
  KEY `fkDetalles_Ventas_Productos` (`Productos_id_producto`),
  CONSTRAINT `fkDetalles_Ventas_Productos` FOREIGN KEY (`Productos_id_producto`) REFERENCES `productos` (`id_producto`),
  CONSTRAINT `fkDetalles_Ventas_Ventas` FOREIGN KEY (`Ventas_id_ventas`) REFERENCES `ventas` (`id_ventas`)
) ENGINE=InnoDB AUTO_INCREMENT=19 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci


